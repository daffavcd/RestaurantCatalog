{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["google", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": 2,
    "require-jsdoc": "off",
    "new-cap": "off"
  }
}
